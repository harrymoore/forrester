{
    "type": "artists-list",
    "config": {
        "chrome": false,
        "buttons": [{
            "key": "create-artist",
            "title": "Create Artist",
            "iconClass": "fa fa-plus",
            "align": "left",
            "action": "create-artist",
            "allowPermission": [
                "create"
            ]
        }, {
            "key": "import-artist",
            "title": "Import Artist",
            "iconClass": "fa fa-plus",
            "align": "left",
            "action": "import-artist",
            "allowPermission": [
                "create"
            ]
        }],
        "columns": [{
            "title": "Artist",
            "key": "titleDescription"
        }],
        "icon": true,
        "checkbox": true,
        "loader": "gitana",
        "selectorGroups": {
            "multi-documents-action-selector-group": {
                "actions": []
            },
            "sort-selector-group": {
                "fields": [{
                    "key": "title",
                    "title": "Title",
                    "field": "title"
                }, {
                    "key": "createdOn",
                    "title": "Created On",
                    "field": "_system.created_on.ms"
                }, {
                    "key": "createdBy",
                    "title": "Created By",
                    "field": "_system.created_by"
                }, {
                    "key": "modifiedOn",
                    "title": "Modified On",
                    "field": "_system.modified_on.ms"
                }, {
                    "key": "modifiedBy",
                    "title": "Modified By",
                    "field": "_system.modified_by"
                }]
            }
        }
    }
}
